     	 	id="org.seisint.eclide.ui.octaveEditorScope"
      		name="Octclipse Context"
      		parentId="org.eclipse.dltk.ui.scriptEditorScope" />
  	</extension>
  	<extension
      	point="org.eclipse.ui.actionSets">
      	<actionSet
            label="%OctaveActionSet.label"
            description="%OctaveActionSet.description"
            visible="false"
            id="org.seisint.eclide.ui.OctaveActionSet">
      	</actionSet>
  	</extension>
   	<extension
    	point="org.eclipse.ui.actionSets">
      	<actionSet
            label="%OctaveSearchActionSet.label"
            description="%OctaveSearchActionSet.description"
            visible="false"
            id="org.seisint.eclide.ui.SearchActionSet">
	        <menu
	          	label="%SearchMenu.label"
	            path="navigate"
	            id="org.eclipse.search.menu">
				<groupMarker name="dialogOctaveGroup"/>
	        </menu>   
	        <action
	            label="%OpenOctaveSearchPageAction.label"
	            icon="$nl$/icons/obj16/search_obj.gif"
	            class="org.seisint.eclide.internal.ui.actions.OpenOctaveSearchPageAction"
	            menubarPath="org.eclipse.search.menu/dialogTclGroup"
	            id="org.seisint.eclide.ui.actions.OctaveSearchPage">
