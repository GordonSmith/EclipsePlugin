 	</extension>
 	<extension
       	point="org.eclipse.ui.editors.templates">
	    <contextType
	          class="org.seisint.eclide.internal.ui.templates.OctaveUniversalTemplateContextType"
	          id="octaveUniversalTemplateContextType"
	          name="%OctaveTemplateContext.name">
	    </contextType>    
	    <include
	          file="templates/templates.xml"
	          translations="templates/templates.properties">
	    </include> 
 	</extension>
  <extension
        point="org.seisint.eclide.core.codeblockprovider">
        <providerContribution
             natureId="org.seisint.eclide.core.nature">
        	<provider
           		class="org.seisint.eclide.internal.ui.text.folding.OctaveParserCodeBlockProviderFactory"
           		id="org.seisint.eclide.ui.codeblockprovider"
           		name="%CodeBlockProvider.name"
           		description="%CodeBlockProvider.description"
           		priority="0">
     		</provider>
     		<selector
           		class="org.seisint.eclide.internal.ui.text.folding.OctaveCodeBlockProviderSelector">
     		</selector>
        </providerContribution>
  </extension>
  <extension
        point="org.eclipse.ui.intro">
       <intro
             class="org.eclipse.ui.intro.config.CustomizableIntroPart"
             id="org.seisint.eclide.ui.intro">
       </intro>
       <introProductBinding
             introId="org.eclipse.ui.intro.universal"
             productId="org.seisint.eclide.ui.product">
       </introProductBinding>
  </extension>
  <extension
   		point="org.eclipse.ui.intro.configExtension">
      	<configExtension
            configId="org.eclipse.ui.intro.universalConfig"
            content="intro/overviewExtensionContent.xml"/>           
      	<configExtension
            configId="org.eclipse.ui.intro.universalConfig"
            content="intro/tutorialsExtensionContent.xml"/>  
      	<configExtension
            configId="org.eclipse.ui.intro.universalConfig"
            content="intro/samplesExtensionContent.xml"/>
      	<configExtension
            configId="org.eclipse.ui.intro.universalConfig"
            content="intro/webresourcesExtensionContent.xml"/>
        <configExtension
            configId="org.eclipse.ui.intro.universalConfig"
            content="intro/whatsNewExtensionContent.xml"/>
  </extension>
   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            name="%octclipseProductName"
            description="%octclipseProductDesc"
            application="org.eclipse.ui.ide.workbench">
         <property
               name="windowImages"
               value="content/octclipse_1616.gif,content/octclipse_3232.gif,content/octclipse_4848.gif,content/octclipse_6464.gif,content/octclipse_128128.gif"/>
         <property
               name="appName"
               value="Octclipse"/>
         <property
               name="startupForegroundColor"
               value="FFFFFF"/>
         <property
               name="startupProgressRect"
               value="5,275,445,15"/>
         <property
               name="startupMessageRect"
               value="7,252,445,20"/>
         <property
               name="preferenceCustomization"
               value="plugin_customization.ini"/>
         <property
               name="aboutImage"
               value="content/octclipse_lg.gif"/>
         <property
               name="introTitle"
               value="Welcome to Octclipse"/>
      	 <property
               name="introBrandingImage"
               value="product:octclipse.png"/>
      	 <property
               name="introBrandingImageText"
               value="%octclipseProductName"/>
         <property
               name="aboutText"
               value="%octclipseProductBlurb"/>
      </product>
   </extension>
</plugin>
