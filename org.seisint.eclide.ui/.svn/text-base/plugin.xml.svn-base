<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>
   	 <extension
       	point="org.eclipse.ui.perspectives">
    	<perspective
          	class="org.seisint.eclide.ui.OctavePerspective"
          	icon="$nl$/icons/eview16/oct_persp.gif"
          	id="org.seisint.eclide.ui.OctavePerspective"
          	name="%OctavePerspective.name"/>
 	</extension>
   	<extension
    	point="org.eclipse.ui.perspectiveExtensions">
      	<perspectiveExtension
        	targetID="org.eclipse.ui.resourcePerspective">
	        <view
	            ratio="0.5"
	            relative="org.eclipse.ui.views.TaskList"
	            relationship="right"
	            id="org.seisint.eclide.ui.views.SymbolsView">
	        </view>
      	</perspectiveExtension>
   	</extension>
	<extension
      	id="org.seisint.eclide.ui.editor.DocumentSetupParticipant"
      	name="%documentSetupParticipantName"
      	point="org.eclipse.core.filebuffers.documentSetup">
		<participant
        	class="org.seisint.eclide.internal.ui.editor.OctaveDocumentSetupParticipant">
   		</participant>
	</extension>
	<extension
        point="org.eclipse.ui.perspectiveExtensions">
    	<perspectiveExtension
        	targetID="org.eclipse.ui.resourcePerspective">
         	<perspectiveShortcut
            	id="org.seisint.eclide.ui.OctavePerspective">
         	</perspectiveShortcut>
      	</perspectiveExtension>
      	<perspectiveExtension
        	targetID="org.seisint.eclide.ui.OctavePerspective">
	    	<perspectiveShortcut
	        	id="org.seisint.eclide.ui.OctavePerspective">
	        </perspectiveShortcut>
      	</perspectiveExtension>
      	<perspectiveExtension
           	targetID="org.seisint.eclide.ui.OctavePerspective">
         	<perspectiveShortcut
               	id="org.eclipse.debug.ui.DebugPerspective">
         	</perspectiveShortcut>
      	</perspectiveExtension>
      	<perspectiveExtension
            targetID="org.eclipse.debug.ui.DebugPerspective">
         	<perspectiveShortcut
               	id="org.seisint.eclide.ui.OctavePerspective">
         	</perspectiveShortcut>
      	</perspectiveExtension>
      	<perspectiveExtension
            targetID="org.eclipse.debug.ui.DebugPerspective">
         	<actionSet
               	id="org.eclipse.jdt.ui.DLTKActionSet">
         	</actionSet>
      	</perspectiveExtension>
      	<perspectiveExtension
            targetID="org.seisint.eclide.ui.OctavePerspective">
         	<showInPart
               	id="org.eclipse.dltk.ui.ScriptExplorer">
         	</showInPart>
      	</perspectiveExtension>
      	<perspectiveExtension
            targetID="org.eclipse.debug.ui.DebugPerspective">
         	<showInPart
               id="org.eclipse.dltk.ui.ScriptExplorer">
         	</showInPart>
      	</perspectiveExtension>  
   	</extension>
	<extension
        point="org.eclipse.ui.newWizards">
      	<category
            id="org.seisint.eclide.ui"
            name="%OctaveWizardsCategory.name">
      	</category>
      	<wizard
            category="org.seisint.eclide.ui"
            class="org.seisint.eclide.internal.ui.wizards.OctaveNewProjectWizard"
            finalPerspective="org.seisint.eclide.ui.OctavePerspective"
            icon="$nl$/icons/etool16/newprj_wiz.gif"
            id="org.seisint.eclide.ui.wizards.OctaveNewProjectWizard"
            name="%NewOctaveProjectWizard.label"
            preferredPerspectives="org.seisint.eclide.ui.OctavePerspective"
            project="true">
         	<description>
            	%NewOctaveProjectWizard.description
         	</description>
      	</wizard>
      	<wizard
            category="org.seisint.eclide.ui"
            class="org.eclipse.dltk.ui.wizards.NewSourceFolderCreationWizard"
            icon="$nl$/icons/etool16/newpackfolder_wiz.gif"
            name="%NewSourceFolderCreationWizard.label"
            id="org.seisint.eclide.ui.wizards.NewSourceFolderCreationWizard">
         	<description>
            	%NewSourceFolderCreationWizard.description
         	</description>
      	</wizard>
       	<wizard
        	category="org.seisint.eclide.ui"
            class="org.seisint.eclide.internal.ui.wizards.OctaveNewFileWizard"
            icon="$nl$/icons/etool16/newfile_wiz.gif"
            id="org.seisint.eclide.ui.wizards.OctaveNewFileWizard"
            name="%NewOctaveFile.label">
          	<description>
            	%NewOctaveFile.description
         	</description>
       	</wizard>
    </extension>
	<extension
       	point="org.eclipse.dltk.ui.language">
    	<language
          	class="org.seisint.eclide.internal.ui.OctaveUILanguageToolkit"
          	nature="org.seisint.eclide.core.nature"/>
 	</extension>
 	<extension
        point="org.eclipse.ui.editors">
      	<editor
            class="org.seisint.eclide.internal.ui.editor.OctaveEditor"
            contributorClass="org.eclipse.dltk.internal.ui.editor.SourceModuleEditorActionContributor"
            default="true"
            extensions="ecl"
            filenames="octaverc,.octaverc,PKG_ADD"
            icon="$nl$/icons/obj16/oct_obj.gif"
            id="org.seisint.eclide.ui.editor.OctaveEditor"
            name="%OctaveEditor.name">
        	<contentTypeBinding
        		contentTypeId="org.seisint.eclide.core.octaveContentType">
        	</contentTypeBinding>
      	</editor>
	</extension>
	<extension
       	point="org.eclipse.ui.preferencePages">
       	<page          
          	class="org.seisint.eclide.internal.ui.preferences.OctaveGlobalPreferencesPage"
          	id="org.seisint.eclide.ui.preferences"
          	name="%OctavePreferencePage.name"/>
    	<page
          	category="org.seisint.eclide.ui.preferences"
          	class="org.seisint.eclide.internal.ui.preferences.OctaveEditorPreferencesPage"
          	id="org.seisint.eclide.ui.preferences.editor"
          	name="%OctaveEditorPreferencePage.name"/>
    	<page
           	category="org.seisint.eclide.ui.preferences.editor"
           	class="org.seisint.eclide.internal.ui.templates.OctaveTemplatePreferencePage"
           	id="org.seisint.eclide.ui.preferences.editor.templates"
           	name="%OctaveCodeTemplatesPreferencePage.name"/>      
    	<page
          	category="org.seisint.eclide.ui.preferences.editor"
          	class="org.seisint.eclide.internal.ui.preferences.OctaveEditorSyntaxColoringPreferencePage"
          	id="org.seisint.eclide.ui.preferences.editor.syntaxcoloring"
          	name="%OctaveEditorSyntaxColoringPreferencePage.name"/>
        <page
            category="org.seisint.eclide.ui.preferences.editor"
            class="org.eclipse.dltk.ui.preferences.ScriptEditorHoverPreferencePage:org.seisint.eclide.core.nature"
            id="org.seisint.eclide.ui.preferences.editor.hovers"
            name="%OctaveEditorHoversPreferencePage.name"/>            	
    	<page
          	category="org.seisint.eclide.ui.preferences.editor"
          	class="org.seisint.eclide.internal.ui.preferences.OctaveSmartTypingPreferencePage"
          	id="org.seisint.eclide.ui.preferences.editor.smarttyping"
          	name="%OctaveEditorTypingPreferencePage.name"/>          
    	<page
          	category="org.seisint.eclide.ui.preferences.editor"
          	class="org.seisint.eclide.internal.ui.preferences.OctaveFoldingPreferencePage"
          	id="org.seisint.eclide.ui.preferences.editor.folding"
          	name="%OctaveEditorFoldingPreferencePage.name"/>
		<page
	      	category="org.seisint.eclide.ui.preferences"
	      	class="org.seisint.eclide.internal.ui.preferences.OctaveParserPreferencePage"
	      	id="org.seisint.eclide.ui.preferences.parser"
	      	name="%OctaveParsersPreferencePage.name" />         
	    <page
  			category="org.seisint.eclide.ui.preferences"
  			class="org.seisint.eclide.internal.ui.preferences.OctaveTodoTaskPreferencePage"
  			id="org.seisint.eclide.ui.preferences.todo"
  			name="%OctaveTaskTags.name"/>
  		<page
  			category="org.seisint.eclide.ui.preferences.editor.folding"
  			class="org.seisint.eclide.internal.ui.preferences.OctaveFoldingProviderPreferencePage"
  			id="org.seisint.eclide.ui.preferences.editor.folding.codeblockprovider"
  			name="%OctaveFoldingProvider.name"/> 	 	
    </extension>   
    <extension
       	point="org.eclipse.ui.propertyPages">
       	<page
      		class="org.seisint.eclide.internal.ui.preferences.OctavePropertyPage"
      		id="org.seisint.eclide.ui.propertyPage"
      		name="%OctavePropertyPage.name">
      		<enabledWhen>
        		<adapt type="org.eclipse.core.resources.IProject">
          			<test property="org.eclipse.core.resources.projectNature" value="org.seisint.eclide.core.nature"/>
        		</adapt>         
      		</enabledWhen>
    	</page>    
       	<page
       		category="org.seisint.eclide.ui.propertyPage"
          	class="org.seisint.eclide.internal.ui.preferences.OctaveBuildPathsPropertyPage"
          	id="org.seisint.eclide.ui.BuildpathProperties"
          	name="%OctaveBuildPathPropertyPage.name">
          	<enabledWhen>
				<adapt type="org.eclipse.core.resources.IProject">
          			<test property="org.eclipse.core.resources.projectNature" value="org.seisint.eclide.core.nature"/>
        		</adapt>
          	</enabledWhen>
       		<filter
             	name="nature"
             	value="org.seisint.eclide.core.nature"/>
       		<keywordReference id="org.eclipse.dltk.ui.buildpath" />
    	</page>	
    	<page
          category="org.seisint.eclide.ui.propertyPage"
          class="org.seisint.eclide.internal.ui.preferences.OctaveTodoTaskPreferencePage"
          id="org.seisint.eclide.ui.propertyPage.todo"
          name="%OctaveTodoPropertyPage.name">
      		<enabledWhen>
        		<adapt type="org.eclipse.core.resources.IProject">
          			<test property="org.eclipse.core.resources.projectNature" value="org.seisint.eclide.core.nature"/>
		        </adapt>         
      		</enabledWhen>
    	</page>  
    </extension>
    <extension
        point="org.eclipse.ui.decorators">
    	<decorator
            adaptable="false"
            location="TOP_RIGHT"
            lightweight="true"
            label="%OctaveProjectDecorator.name"
            state="true"
            class="org.seisint.eclide.internal.ui.OctaveProjectDecorator"
            id="org.seisint.eclide.ui.decorators.projectdecorator">
         	<enablement>
	           	<or>
	            	<objectClass name="org.eclipse.core.resources.IProject"/>
	            	<objectClass name="org.eclipse.dltk.core.IScriptProject"/>
	           	</or>
         	</enablement>
      	</decorator>
   	</extension>
   	<extension
		point="org.eclipse.dltk.ui.scriptCompletionProposalComputer"
		id="OctaveCompletionProposalComputer">
		<scriptCompletionProposalComputer 
			class="org.seisint.eclide.internal.ui.text.completion.OctaveCompletionProposalComputer"
			categoryId="org.eclipse.dltk.ui.scriptTypeProposalCategory"
			toolkitId="org.seisint.eclide.core.nature">
			<partition type="__dftl_partition_content_type"/>
		</scriptCompletionProposalComputer>	
	</extension>
	<extension point="org.eclipse.core.runtime.preferences">
        <initializer class="org.seisint.eclide.internal.ui.OctaveUIPreferenceInitializer"/>
    </extension>	
    <extension
        point="org.eclipse.ui.actionSetPartAssociations">
        <actionSetPartAssociation
            targetID="org.seisint.eclide.ui.CodingActionSet">
         <part id="org.seisint.eclide.ui.editor.OctaveEditor"/>
	     <part id="org.eclipse.dltk.ui.ScriptExplorer"/>
        </actionSetPartAssociation>
        <actionSetPartAssociation
            targetID="org.eclipse.ui.edit.text.actionSet.presentation">
            <part id="org.seisint.eclide.ui.editor.OctaveEditor"/>
        </actionSetPartAssociation>
        <actionSetPartAssociation
            targetID="org.eclipse.dltk.ui.A_OpenActionSet">
            <part id="org.seisint.eclide.ui.editor.OctaveEditor"/>
        </actionSetPartAssociation>
        <actionSetPartAssociation
            targetID="org.eclipse.dltk.ui.ScriptCodingActionSet">
            <part id="org.seisint.eclide.ui.editor.OctaveEditor"/>
        </actionSetPartAssociation>
		<actionSetPartAssociation
            targetID="org.seisint.eclide.ui.SearchActionSet">
            <part id="org.eclipse.dltk.ui.ScriptExplorer"/>
            <part id="org.seisint.eclide.ui.editor.OctaveEditor"/>
            <part id="org.eclipse.search.SearchResultView"/>
        </actionSetPartAssociation>
    </extension> 
    <extension
        point="org.eclipse.ui.actionSets">
        <actionSet
            label="%OctaveSearchActionSet.label"
            description="%OctaveSearchActionSet.description"
            visible="false"
            id="org.seisint.eclide.ui.SearchActionSet">
            <menu
                label="%SearchMenu.label"
                path="navigate"
                id="org.eclipse.search.menu">
                <groupMarker name="dialogOctaveGroup"/>
            </menu>   
            <action
                label="%OpenOctaveSearchPageAction.label"
                icon="$nl$/icons/obj16/search_obj.gif"
                class="org.seisint.eclide.internal.ui.actions.OpenOctaveSearchPageAction"
                menubarPath="org.eclipse.search.menu/dialogOctaveGroup"
                id="org.seisint.eclide.ui.actions.OpenOctaveSearchPage">
            </action>
        </actionSet>
       <actionSet
            label="%ScriptCodingActionSet.label"
            description="%ScriptCodingActionSet.description"
            visible="false"
            id="org.seisint.eclide.ui.CodingActionSet">
         <menu
               label="%SourceMenu.label"
               path="edit"
               id="org.eclipse.dltk.ui.source.menu">
		    <separator
                  name="commentGroup">
            </separator>
            <separator
                  name="editGroup">
            </separator>
            <separator
                  name="importGroup">
            </separator>
            <separator
                  name="generateGroup">
            </separator>
            <separator
                  name="codeGroup">
            </separator>
            <separator
                  name="externalizeGroup">
            </separator>
            <separator
                  name="convertGroup">
            </separator>
         </menu>
         <action
               definitionId="org.eclipse.dltk.ui.edit.text.script.quick.format"
               label="%FormatElementAction.label"
               retarget="true"
               menubarPath="org.eclipse.dltk.ui.source.menu/editGroup"
               id="org.eclipse.dltk.ui.actions.FormatElement">
         </action>
         <action
               definitionId="org.eclipse.dltk.ui.edit.text.script.format"
               label="%FormatAction.label"
               retarget="true"
               menubarPath="org.eclipse.dltk.ui.source.menu/editGroup"
               id="org.eclipse.dltk.ui.actions.Format">
         </action>
         <action
               definitionId="org.eclipse.dltk.ui.edit.text.script.indent"
               label="%IndentAction.label"
               retarget="true"
               menubarPath="org.eclipse.dltk.ui.source.menu/editGroup"
               id="org.eclipse.dltk.ui.actions.Indent">
         </action>
         <action
               definitionId="org.eclipse.ui.edit.text.shiftLeft"
               label="%ShiftLeftAction.label"
               retarget="true"
               menubarPath="org.eclipse.dltk.ui.source.menu/editGroup"
               id="org.eclipse.dltk.ui.actions.ShiftLeft">
         </action>
         <action
               definitionId="org.eclipse.ui.edit.text.shiftRight"
               label="%ShiftRightAction.label"
               retarget="true"
               menubarPath="org.eclipse.dltk.ui.source.menu/editGroup"
               id="org.eclipse.dltk.ui.actions.ShiftRight">
         </action>
         <action
               definitionId="org.eclipse.dltk.ui.edit.text.script.uncomment"
               label="%UncommentAction.label"
               retarget="true"
               id="org.eclipse.dltk.ui.actions.Uncomment">
         </action>
         <action
               definitionId="org.eclipse.dltk.ui.edit.text.script.comment"
               label="%CommentAction.label"
               retarget="true"
               id="org.eclipse.dltk.ui.actions.Comment">
         </action>
		<action
               definitionId="org.eclipse.dltk.ui.edit.text.script.remove.block.comment"
               label="%RemoveBlockCommentAction.label"
               retarget="true"
               menubarPath="org.eclipse.dltk.ui.source.menu/commentGroup"
               id="org.eclipse.dltk.ui.actions.RemoveBlockComment">
         </action>
         <action
               definitionId="org.eclipse.dltk.ui.edit.text.script.add.block.comment"
               label="%AddBlockCommentAction.label"
               retarget="true"
               menubarPath="org.eclipse.dltk.ui.source.menu/commentGroup"
               id="org.eclipse.dltk.ui.actions.AddBlockComment">
         </action>
      </actionSet>
    </extension> 
    <extension
        point="org.eclipse.search.searchPages">
        <page
            canSearchEnclosingProjects="true"
            class="org.seisint.eclide.internal.ui.search.OctaveSearchPage"
            enabled="true"
            extensions="m:100"
            icon="$nl$/icons/obj16/search_obj.gif"
            id="org.seisint.eclide.ui.OctaveSearchPage"
            label="%OctaveSearchPage.label"
            showScopeSection="true"
            sizeHint="460,160">
        </page>
    </extension>
    <extension
        point="org.eclipse.dltk.ui.scriptDocumentationProviders">
        <provider
            class="org.seisint.eclide.internal.ui.documentation.OctaveScriptDocumentationProvider"
            id="org.seisint.eclide.ui.documentation.OctaveScriptDocumentationProvider"
            nature="org.seisint.eclide.core.nature"/>
        <provider
            class="org.seisint.eclide.internal.ui.documentation.OctaveSymbolDocumentationProvider"
            id="org.seisint.eclide.ui.documentation.OctaveSymbolDocumentationProvider"
            nature="org.seisint.eclide.core.nature"/>
    </extension>
    <extension
        point="org.eclipse.ui.views">
        <category
            id="org.seisint.eclide.ui.views"
            name="%OctaveViewsCategory.name"/>
        <view
            name="%OctaveDocumentationView.name"
            icon="$nl$/icons/obj16/octdoc.gif"
            category="org.seisint.eclide.ui.views"
            class="org.seisint.eclide.internal.ui.infoviews.OctaveDocumentationView"
            id="org.seisint.eclide.ui.OctaveDocumentationView" />
    </extension>
    <extension
    	point="org.eclipse.ui.contexts">
