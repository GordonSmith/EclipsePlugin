</template>

<template 
	context="rubyUniversalTemplateContextType" 
	description="%elseTemplate1.description" 
	id="org.seisint.eclide.ui.templates.else1" 
	name="else">else
  ${cursor}
</template>

<template 
	context="octaveUniversalTemplateContextType" 
	description="%switchTemplate1.description" 
	id="org.seisint.eclide.ui.templates.switch1" 
	name="switch">switch ( ${expression} )
  case ${case1}
    ${case1_body}
  otherwise
    ${otherwise_body}
endswitch</template>

<template 
	context="octaveUniversalTemplateContextType" 
	description="%caseTemplate1.description" 
	id="org.seisint.eclide.ui.templates.case1" 
	name="case">case ${case1}
  ${cursor}
</template>

<template 
	context="octaveUniversalTemplateContextType" 
	description="%unwindTemplate1.description" 
	id="org.seisint.eclide.ui.templates.unwind_protect1" 
	name="unwind_protect">unwind_protect
  ${unwind_protect_body}
unwind_protect_cleanup
  ${unwind_protect_cleanup_body}
end_unwind_protect</template>

<template 
	context="octaveUniversalTemplateContextType" 
	description="%tryTemplate1.description" 
	id="org.eclipse.dltk.ruby.ui.templates.try_catch1" 
	name="try">try
  ${try_body}
catch
  ${catch_body}
end_try_catch</template>

