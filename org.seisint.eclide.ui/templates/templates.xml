<template 
	context="octaveUniversalTemplateContextType" 
	description="%ifelseTemplate1.description" 
	id="org.seisint.eclide.ui.templates.ifelse1" 
	name="ifelse">if ( ${condition} )
  ${if_body}
else
  ${else_body}
endif</template>

<template 
	context="octaveUniversalTemplateContextType" 
	description="%elseifTemplate1.description" 
	id="org.seisint.eclide.ui.templates.elsif1" 
	name="elseif">elseif ( ${condition} )
