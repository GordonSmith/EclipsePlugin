<?xml version="1.0" encoding="UTF-8"?>
<templates>

<template 
	context="octaveUniversalTemplateContextType" 
	description="%functionTemplate1.description" 
	id="org.seisint.eclide.ui.templates.function1" 
	name="function">function ${file_as_function} ()
  ${cursor}
endfunction</template>

<template 
	context="octaveUniversalTemplateContextType" 
	description="%functionTemplate2.description"
	id="org.seisint.eclide.ui.templates.function2" 
	name="function">function [ ${argout} ] = ${file_as_function} ( ${argin} )
  ${cursor}
endfunction</template>

<template 
	context="octaveUniversalTemplateContextType" 
	description="%forTemplate1.description" 
	id="org.seisint.eclide.ui.templates.for1" 
	name="for">for ${var} = ${start}:${stop}
  ${cursor}
endfor</template>

<template 
	context="octaveUniversalTemplateContextType" 
	description="%forTemplate2.description" 
	id="org.seisint.eclide.ui.templates.for2" 
	name="for">for ${var} = ${expression}
  ${cursor}
endfor</template>

<template 
	context="octaveUniversalTemplateContextType" 
	description="%whileTemplate1.description" 
	id="org.seisint.eclide.ui.templates.while1" 
	name="while">while ( ${condition} )
  ${cursor}
endwhile</template>

<template 
	context="octaveUniversalTemplateContextType" 
	description="%dountilTemplate1.description" 
	id="org.seisint.eclide.ui.templates.dountil1" 
	name="do">do
  ${cursor}
until ( ${condition} )</template>

<template 
	context="octaveUniversalTemplateContextType" 
	description="%ifTemplate1.description" 
	id="org.seisint.eclide.ui.templates.if1" 
	name="if">if ( ${condition} )
  ${cursor}
