<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
    <extension-point id="octavekeywords" name="octavekeywords" schema="schema/octavekeywords.exsd"/>
    <extension-point id="codeblockprovider" name="codeblockprovider" schema="schema/codeblockprovider.exsd"/>
	<extension
         id="org.seisint.eclide.core.nature"
         point="org.eclipse.core.resources.natures">
      	<runtime>
         	<run class="org.seisint.eclide.core.OctaveNature"/>
      	</runtime>
   	</extension>i
   	<extension
         point="org.eclipse.dltk.core.language">
      <language
            class="org.seisint.eclide.core.OctaveLanguageToolkit"
            nature="org.seisint.eclide.core.nature"/>
   	</extension> 
   	<extension
         point="org.eclipse.dltk.core.sourceElementParsers">
      <parser
            class="org.seisint.eclide.internal.parser.OctaveSourceElementParser"
            nature="org.seisint.eclide.core.nature"
            priority="0">
      </parser>
   	</extension>
   	<extension
         point="org.eclipse.dltk.core.selectionEngine">
      <selectionEngine
            class="org.seisint.eclide.internal.codeassist.OctaveSelectionEngine"
            nature="org.seisint.eclide.core.nature"
            priority="0">
      </selectionEngine>
   	</extension>
   	<extension
         point="org.eclipse.dltk.core.completionEngine">
      <completionEngine
            class="org.seisint.eclide.internal.codeassist.OctaveCompletionEngine"
            nature="org.seisint.eclide.core.nature"
            priority="0">
      </completionEngine>
   	</extension>
   	<extension
         point="org.eclipse.dltk.core.search">
      <seachFactory
            class="org.seisint.eclide.core.OctaveSearchFactory"
            nature="org.seisint.eclide.core.nature"
            priority="0">
      </seachFactory> 
      <matchLocator
            class="org.seisint.eclide.internal.search.MatchLocator"
            nature="org.seisint.eclide.core.nature">
      </matchLocator>
   	</extension>
   	<extension
         point="org.eclipse.dltk.core.callHierarchy">
      <callHerarchiesFactory
            class="org.seisint.eclide.internal.core.OctaveCallHierarchyFactory"
            nature="org.seisint.eclide.core.nature"
            priority="0">
      </callHerarchiesFactory>
   	</extension>
   	<extension
        point="org.eclipse.core.runtime.contentTypes">
     <content-type
           base-type="org.eclipse.core.runtime.text"
           describer="org.seisint.eclide.core.OctaveContentDescriber"
           file-extensions="ecl"
           id="org.seisint.eclide.core.octaveContentType"
           name="%octaveContentType"
           priority="high">       
     </content-type>
  	</extension>
  	<extension
         point="org.eclipse.dltk.core.sourceParsers">
      	<parserContribution
        	natureId="org.seisint.eclide.core.nature">
         	<parser
            	class="org.seisint.eclide.internal.parser.OctaveSourceParserFactory"
            	id="org.seisint.eclide.core.sourceParser"
            	description="%sourceParser.description"
           		name="%sourceParser.name"
            	priority="0" />
        	<selector
        		class="org.seisint.eclide.internal.parser.OctaveSourceParserSelector" />
      	</parserContribution>
   	</extension>
   	<extension
        point="org.seisint.eclide.core.octavekeywords">
      	<keywords
            class="org.seisint.eclide.core.OctaveKeywords"
            priority="0">
      	</keywords>
   	</extension>
   	<extension
    	point="org.eclipse.dltk.core.buildParticipant">
      	<buildParticipant
            class="org.eclipse.dltk.core.builder.ParserBuildParticipantFactory"
            id="org.seisint.eclide.core.buildParticipant.parser"
            name="%parserBuildParticipant.name"
            nature="org.seisint.eclide.core.nature">
      	</buildParticipant>
    	<buildParticipant
      		class="org.seisint.eclide.internal.parser.OctaveTodoParserType"
      		id="org.seisint.eclide.todo"
      		name="%taskTagsBuildParticipant.name"
      		nature="org.seisint.eclide.core.nature">
      		<requires
            	id="org.seisint.eclide.core.buildParticipant.parser">
      		</requires>
    	</buildParticipant>
  	</extension>
  	<extension point="org.eclipse.core.runtime.preferences">
        <initializer class="org.seisint.eclide.internal.core.OctclipseCorePreferenceInitializer"/>
   	</extension>
</plugin>
